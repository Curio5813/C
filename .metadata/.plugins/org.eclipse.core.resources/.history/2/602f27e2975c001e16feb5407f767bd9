#include <stdio.h>


int main(){

	int num;
	float val;
	char carac;
	char strg[50];

	scanf("%d%f%c%s[^\n]",&num, &val, &carac, &strg);
	printf("%d%.6f%s%s\n", num, val, carac, strg);
	printf("%d\t%.6f\t%s\t%s\n", num, val, carac, strg);
	printf("%10d%10.6f%10s%10s\n", num, val, carac, strg);


return 0;

}
