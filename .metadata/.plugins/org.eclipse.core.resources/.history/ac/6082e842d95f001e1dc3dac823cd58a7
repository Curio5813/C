#include <stdio.h>


void find_3_numbers(){

	long long int x, y, z, cont = 1, bol = 0, a, b, c, prod, n[4] = {3, 100, 35, 3485};

	long long int r_a, r_b, r_c, r_p;

	while(cont <= 3){

		for(x = 1; x <= n[cont]; x++)
			printf("%lld\n", x);
			for(y = 1; y <= n[cont]; y++)

				for(z = 1; z <= n[cont]; z++)

					if(x != y && x != z && y != z){

						a = x * y;
						b = x * z;
						c = y * z;

						prod = x * y * z;

						r_a = n[cont] % a;
						r_b = n[cont] % b;
						r_c = n[cont] % c;
						r_p = prod % n[cont];


						if(r_a == 0 && r_b == 0 && r_c == 0 && r_p == 0){

							printf("%lld %lld %lld ", x, y, z);
							cont += 1;
							break;
						}



		}printf("%d ", 0);
		cont += 1;

	}


}


int main(){

	find_3_numbers();

	return 0;

}
