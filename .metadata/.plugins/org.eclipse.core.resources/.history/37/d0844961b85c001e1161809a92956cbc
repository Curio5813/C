#include <stdio.h>


int main(){

	long long int cont, soma, i, k, j, n1, n2;

	int data[2][3] = {{1, 1, 5}, {3, 5, 4}};

	for(i = 0; i < 2; i++){
		cont = 0, soma = 0, n1 = 1, n2 = 1;
		while(soma < data[i][2]){
			cont += 1;
			if(cont == data[i][0] * n1){
				soma += 1;
				if(soma == data[i][2]){
					printf("%d ", cont);
					break;

				}
			}if(cont == data[i][1]){
				soma += 1;
				if(soma == data[i][2] * n2){
					printf("%d ", cont);
					break;
				}
			}
		}
	}

	return 0;

}
