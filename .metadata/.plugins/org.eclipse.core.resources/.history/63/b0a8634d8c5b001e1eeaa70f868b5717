#include <stdio.h>


long long int cont = 0, l, i, num;

char k[100], n[100];

int  main(){


	while(cont < 20){

		for(i = 0; i < 20; i++){
			if(i == 0){
				scanf("%s %lld", &k[i], &l);
			}else if(i > 0 && i < 3){
				scanf("%s", &k[i]);

		for(i = 0; i < 20; i++){
			n[i] += k[i];

		}num = atoi(n);
		printf("%dll", num);

		for(i = 2; i < 20; i++){
			if(num % i == 0 && i >= l){
				printf("GOOD\n");
				break;

			}else if(num % i == 0 && i < l){
				printf("BAD %lld\n", i);
				break;
			}


		}cont += 1;
	}

	return 0;

}
