#include <stdio.h>


void vetor_uniao(){

	int a[5], b[5], c[10], i, k, cont = 0;

	for(i = 0; i < 5; i++){
		printf("Digite o %dº inteiro para o vetor A: ", i + 1);
		scanf("%d", &a[i]);

	}printf("\n");
	for(i = 0; i < 5; i++){
		printf("Digite o %dº inteiro para o vetor B: ", i + 1);
		scanf("%d", &b[i]);


	}printf("\n");
	for(i = 0; i < 5; i ++){
		for(k = 0; k < 5; k++){
			if(a[i] == b[k]){
				c[i] = a[i];

			}else if(a[i] != b[i]){
				c[i] = a[i];
				c[i + 1] = b[i];

			}

		}

	}printf("\n");
	for(i = 0; i < 10; i++){
		if(c[i] != 0){
			printf("%d ", c[i]);

		}

	}

}


int main(){

	vetor_uniao();

	return 0;

}
