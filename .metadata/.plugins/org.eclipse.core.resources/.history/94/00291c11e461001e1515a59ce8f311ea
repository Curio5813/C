#include <stdio.h>


int main(){

	int n, pacote[1000], tempo[1000];

	while(scanf("%d", &n) != EOF){

		for(int i = 0; i < n; i++){
			fscanf("%d ", &pacote[i]);

		}for(int i = 0; i < n; i++){
			fscanf("%d ", &tempo[i]);

		}for(int i = 0; i < n; i++){
			printf("%d ", pacote[i]);

		}printf("\n");
		for(int i = 0; i < n; i++){
			printf("%d ", tempo[i]);

		}

	}

	return 0;

}
