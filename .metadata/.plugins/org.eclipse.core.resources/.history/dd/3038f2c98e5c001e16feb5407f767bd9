#include <stdio.h>


int num;
float val;
char c;
char str[50];


void entrada_e_saida(){

	scanf("%d", &num);
	scanf("%f", &val);
	scanf("%c", &c);
	gets(str);
	printf("%d%.6f%s%s\n", num, val, c, str);
	printf("%d\t%.6f\t%s\t%s\n", num, val, c, str);
	printf("%10d%10.6f%10s%10s", num, val, c, str);


}


int main(){

	entrada_e_saida();

	return 0;

}

