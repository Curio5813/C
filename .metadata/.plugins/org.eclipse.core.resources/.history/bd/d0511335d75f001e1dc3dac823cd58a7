#include <stdio.h>


void find_3_numbers(){

	long long int x, y, z, cont = 1, bol = 0, a, b, c, prod, n[4] = {3, 100, 35, 3485};

	while(cont <= 3){

		for(x = 1; x <= n[cont]; x++){

			for(y = 1; y <= n[cont]; y++){

				for(z = 1; z <= n[cont]; z++){

					a = x * y;
					b = x * z;
					c = y * z;

					printf("%lld\n", a);

					prod = x * y * z;

					if(x != y && x != z && y != z && n[cont] % a == 0 && n[cont] % b == 0
						&& n[cont] % c == 0 && prod % n[cont] == 0){

						printf("%lld %lld %lld ", x, y, z);
						cont += 1;
						break;
					}

				}

			}

		}printf("%d ", 0);
		cont += 1;

	}


}


int main(){

	find_3_numbers();

	return 0;

}
