#include <stdio.h>


void vetor_uniao(){

	int a[10], b[10], u[10], i;

	for(i = 0; i < 10; i++){
		printf("Digite o %dÂº inteiro para o vetor A e outro para o vetor B: ", i + 1);
		scanf("%d %d", &a[i], &b[i]);


	}for(i = 0; i < 10; i ++){
		if(a[i] == b[i]){
			u[i] = a[i];

		}else{
			u[i] = 0;

		}
	}for(i = 0; i < 10; i++){
		if(u[i] != 0){
			printf("%d ", u[i]);

		}

	}

}


int main(){

	vetor_uniao();

	return 0;

}
