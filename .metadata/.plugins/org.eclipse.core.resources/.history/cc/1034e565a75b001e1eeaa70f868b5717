#include <stdio.h>


long double cont = 0, k, l;

long int i;


int  main(){


	while(cont < 20){

		if(k == 0 && l == 0){
			break;
		}

		scanf("%lld%lld", &k, &l);

		for(i = 2; i < 1000000 + 1; i++){
			if((long long int) k % i == 0 && i >= l){
				printf("GOOD\n");
				break;

			}else if((long long int)k % i == 0 && i < l){
				printf("BAD %.0llf\n", i);
				break;
			}


		}cont += 1;
	}

	return 0;

}
