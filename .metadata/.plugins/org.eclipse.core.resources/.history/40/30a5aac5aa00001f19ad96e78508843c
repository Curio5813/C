#include <stdio.h>



void bubbleSort(int lados[3]){
    for (int step = 0; step < 3 - 1; ++step) {
        for (int i = 0; i < 3 - step - 1; ++i) {
            // Compara elementos adjacentes e troca se o anterior for maior que o prÃ³ximo
            if(lados[i] > lados[i + 1]) {
            	int temp = lados[i];
                lados[i] = lados[i + 1];
                lados[i + 1] = temp;
            }
        }
    }
}


int main(){

	double lados[3];

	for(int i = 0; i < 3; i++){
		scanf("%lf", lados[i]);
	}

	bubbleSort(lados[3]);

	printf("Vetor ordenado:\n");
	for(int i = 0; i < 3; i++) {
		printf("%d ", lados[i]);
	}
	printf("\n");


	return 0;
}
