#include <stdio.h>


int fat = 1, n = 0;

double e = 0;


void EulerNumber(){

	for(int i = 0; i < 100; i++){
		fat = 1;
		n = 1;
		while(n >= i){
			fat *= n;
			n += 1;
		}e += 1 / (double)fat;


	}printf("O número de Euler é aproximadamente %lf", e);

}


int main(){

	EulerNumber();

	return 0;

}
