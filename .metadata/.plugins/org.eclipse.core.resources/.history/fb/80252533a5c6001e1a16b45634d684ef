#include <stdio.h>


const int MAX = 50005;
int vet[MAX];
int mapa[MAX];

void my_reverse(int vet[], int a, int b){
    while(a!=b && a!=--b){
        swap(vet[a], vet[b]);
        swap(mapa[vet[a]],mapa[vet[b]]);
        a++;
    }
    return;
}

int main(){
    int n, cont = 0;
    while(scanf("%d",&n),n){
        for(int i=0;i<n;i++){
            vet[i] = i + 1;
            mapa[vet[i]] = i + 1;
        }
        int u;
        scanf("%d",&u);
        while(u--){
            int a, b;
            scanf("%d%d",&a,&b);
            my_reverse(vet,a-1,b);
        }
        int x;
        scanf("%d",&x);
        printf("Genome %d\n",++cont);
        while(x--){
            int ind;
            scanf("%d",&ind);
            printf("%d\n",mapa[ind]);
        }
    }
    return 0;
}
