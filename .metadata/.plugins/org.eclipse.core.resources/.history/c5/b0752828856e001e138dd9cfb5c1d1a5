#include <stdio.h>
#include <stdlib.h>


int ordenacao_vetor(int *a, int *b, int *c){

	if(*(a) >= *(b)){

		return printf("%d %d %d", a, b, c);

	}

}


int main(){

	int a, b, c;

	printf("Digite três números inteiros: ");
	scanf("%d %d %d", &a, &b, &c);

	ordenacao_vetor(&a, &b, &c);

	return 0;

}
