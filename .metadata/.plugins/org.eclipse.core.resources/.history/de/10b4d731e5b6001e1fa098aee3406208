int level(){

	int nivel, aleo, vetor1[2];

	printf("Escolha um n√≠vel de dificuldade [1, 2, 3, ou 4]: ", nivel);
	scanf("%d", &nivel);

	if(nivel == 1){

		srand(time(NULL));

		for(int i = 0; i < 2; i++){
			int numero = rand() % 10 + 1;
			vetor1[i] = numero;
		}
		return vetor1;

	}
	else if(nivel == 2){

		srand(time(NULL));

		for(int i = 0; i < 2; i++){
			int numero = rand() % 100 + 1;
			vetor1[i] = numero;
		}
		return vetor1;
	}
	else if(nivel == 3){

		srand(time(NULL));

		for(int i = 0; i < 2; i++){
			int numero = rand() % 100 + 1;
			vetor1[i] = numero;
		}
		return vetor1;
	}
	else if(nivel == 4){

		srand(time(NULL));

		for(int i = 0; i < 2; i++){
			int numero = rand() % 100 + 1;
			vetor1[i] = numero;
		}
		return vetor1;
	}

}

