#include <stdio.h>


int fibo[1	] = {9};

int main(){

	long long int a = 0, b = 1;

	for(int i = 0; i <= 1; i++){
		a = b;
		b = p;
		p = a + b;
		for(int k = 2; k <= 100000; k++){
			a = b;
			b = p;
			p = a + b;
			if(a % fibo[i] == 0){
				printf("%lld ", k + 1);
				break;

			}

		}
	}

	return 0;
}
