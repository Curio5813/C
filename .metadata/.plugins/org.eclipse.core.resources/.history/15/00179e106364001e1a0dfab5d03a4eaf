#include <stdio.h>


int main(){

	char pal[5], sub[2];

	int k = 0, bol = 0;

	printf("Digite uma palavra: ");
	scanf("%s", &pal);
	printf("Digite uma sub-string: ");
	scanf("%s", &sub);
	printf("\n");

	printf("A palavra foi %s\n", pal);
	printf("A sub-string foi %s\n", sub);

	for(int i = 0; i < 5; i++){
		while(k < 5){
			if(sub[k] == pal[k]){
				bol = 1;

			}else if(pal[k] != sub[k]){
				bol = 0;

			}k += 1;
			if(bol == 1 && k == 2){
				return printf("É string '%s' é uma sub-string a palavra %s", sub, pal);
			}

		}k = i;

	}if(bol == 0){
		printf("%d", -1);

	}

	return 0;

}

