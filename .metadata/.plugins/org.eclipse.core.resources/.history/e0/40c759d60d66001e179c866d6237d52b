#include <stdio.h>
#include <string.h>


int main(){

	char palavra[50], anagrama[50];

	int i, k = i, cont = 0, n1 = 0, n2 = 0;

	printf("Digite uma palavra: ");
	scanf("%s", palavra);
	printf("Digite outra palavra: ");
	scanf("%s", anagrama);
	printf("\n");
	printf("%d", strlen(palavra));
	printf("\n");


	for(i = 0; i < 50; i++){
		while(k < 50){
			if(palavra[n2] == anagrama[n1]){
				cont += 1;
				break;

			}k += 1;
			n1 += 1;

		}n2 = i;
		n1 = 0;
		k = 0;

	}if(cont >= 50){
		return printf("É uma anagrama da palavra %s", palavra);

	}if(cont != 5){
		printf("Não é um anagrama da palavra %s\n", palavra);

	}

	return 0;

}

