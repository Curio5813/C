#include <stdio.h>


int main(){

	int n, pacote[1000], tempo[1000];

		scanf("%d", &n);

		scanf("%d ", &pacote[0]);

		scanf("%d ", &tempo[0]);

		for(int i = 0; i < n; i++){
			printf("%d ", pacote[i]);

		}printf("\n");
		for(int i = 0; i < n; i++){
			printf("%d ", tempo[i]);

		}


	return 0;

}
