#include <stdio.h>


int main(){

	long long int x, y, n, time = 0, i, k;

	int data[2][3] = {{1, 1, 5}, {3, 5, 4}};

	for(i = 0; i < 2; i++){
		for(k = 0; k < 3; k++){

			time += data[i][0];
			if(time == data[i][2]){
				printf("%lld ", time);

			}time += data[i][1];
			if(time == data[i][2]){
				printf("%lld ", time);

			}
		}
	}

	return 0;

}
