#include <stdio.h>
#include <stdlib.h>
#include <string.h>


void string_dois(){

	char str1[50], str2[50];

	int n, tam1, tam2, idx = 0;

	tam1 = strlen(str1);

	tam2 = tam1 + n;

	printf("Digite a primeira string: ");
	scanf("%s", &str1);
	printf("Digite a segunda string: ");
	scanf("%s", &str2);
	printf("\n");

	for(int i = 0; i < tam2; i++){
		str1[tam1] = str2[i];
		tam1 += 1;

	}printf("%s", str1);

}


int main(){

	string_dois();

	return 0;

}
