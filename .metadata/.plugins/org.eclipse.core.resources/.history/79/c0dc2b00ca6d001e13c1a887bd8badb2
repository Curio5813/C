#include <stdio.h>
#include <stdlib.h>


int enderecos_pares(int *v){

	for(int i = 0; i < 5; i++){

		if(v[i] % 2 == 0){

			printf("%d", &v[i]);
		}

	}

}


int main(){

	int v[5];

	for(int i = 0; i < 5; i ++){

		printf("Digite o %dÂº valor do array: ", i + 1);
		scanf("%d", v[i]);

	}printf("\n");

	enderecos_pares(&v);

	return 0;
}
