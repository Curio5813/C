#include <stdio.h>
#include <math.h>


int n, i = 1;

double x, rad, seno, pi = 3.141593, fat;


int SerieDeTaylor(){

	printf("Digite o valor de angulo em graus: ");
	scanf("%lf", &x);
	printf("\n");

	rad = (x * pi) / 180.0;

	printf("%.5lf", rad);
	printf("\n");


	while(i <= 11){

		fat *= i;
		i += 1;

	}printf("%.0lf", fat);
	printf("\n");

	seno = pow(-1, 5) / (fat) * (pow(rad, 11));
	printf("%.5lf", seno);


}


int main(){

	SerieDeTaylor();

	return 0;

}
