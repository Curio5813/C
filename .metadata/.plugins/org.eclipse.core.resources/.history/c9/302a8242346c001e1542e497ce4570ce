#include <stdio.h>
#include <stdlib.h>


void endereco_de_memoria(){

	int n1, n2;

	printf("Digite dois números inteiros: ");
	scanf("%d %d", &n1, &n2);
	printf("\n");

	int *p1 = &n1, *p2 = &n2;

	printf("O endereço de memória do número %d é %p1\ne do número %d é %p2", n1, p1, n2, p2);
	printf("\n\n");

	printf("Digite os dois endereços obitidos por cada variável: ");
	scanf("%d %d", p1, p2);
	printf("\n");

	if(p1 > p2){
		printf("O número que está no maior endereço de memória é %d", n1);

	}else if(p1 < p2){
		printf("O número que está no maior endereço de memória é %d", n2);

	}

}


int main(){

	endereco_de_memoria();

	return 0;

}
