#include <stdio.h>


int main(){

	long long int num, i;

	printf("Digite um número: ");
	scanf("%lld", &num);
	for(i = 2; i * i <= num; i++){
		if(num % i == 0){
			printf("O número %lld não é primo.\n", num);
			printf("O número é divisível por %lld.", i);
			break;
		}
		if(i * i >= num){
			printf("O número %lld é primo.", num);
		}
	}

	return 0;
}
