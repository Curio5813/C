#include <stdio.h>
#include <math.h>


int main(){

	int num, div;

	float raiz;

	printf("Digite um número: ");
	scanf("%d", &num);
	raiz = sqrt(num);
	div = ceil(raiz);
	for(int i = 2; i <= raiz; i++){
		if(i == div && (int)raiz % div == 0){
			printf("O número %d não é primo.\n", num);
			printf("O número %d é divisível por %d.", num, i);
			break;
		}
		if(i == div && (int)raiz != div){
			printf("O número %d é primo.", num);
			break;
		}
	}

	return 0;
}
