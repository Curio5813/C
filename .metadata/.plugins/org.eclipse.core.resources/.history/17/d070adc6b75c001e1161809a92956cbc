#include <stdio.h>


int main(){

	long long int cont, soma, i, k, j;

	int data[2][3] = {{1, 1, 5}, {3, 5, 4}};

	for(i = 0; i < 2; i++){
		cont = 0, soma = 0;
		while(soma < data[i][2]){
			cont += 1;
			if(cont == data[i][0]){
				soma += 1;
				if(soma == data[i][2]){
					printf("lld ", cont);
					break;

				}
			}if(cont == data[i][1]){
				soma += 1;
				if(soma == data[i][2]){
					printf("lld ", cont);
					break;
				}
			}
		}
	}

	return 0;

}
