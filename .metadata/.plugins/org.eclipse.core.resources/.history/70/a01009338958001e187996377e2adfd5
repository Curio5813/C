#include <stdio.h>


int n, num = 2;


int NumerosPrimos(){

	printf("Digite um n√∫mero para n: ");
	scanf("%d", &n);

	while(num <= n){

		for(int i = 2; i <= num; i++){

			if(num % i == 0 && num == i){

				printf("%d ", num);

			}num += 1;
			i = 2;
		}

	}

}


int main(){

	NumerosPrimos();

	return 0;

}

