#include <stdio.h>
#include <stdlib.h>


int array_e_ponteiros(int *v, *valor);

	v[0] = *(valor);
	++valor;
	v[1] = *(valor);
	++valor;
	v[2] = *(valor);
	++valor;
	v[3] = *(valor);
	++valor;
	v[4] = *(valor);
	++valor;


int main(){

	int v[5], valor = 5;

	array_e_ponteiros(&v, &valor);

	for(int i = 0; i < 5; i++){

		printf("%d ", v[i]);

	}

	return 0;

}
