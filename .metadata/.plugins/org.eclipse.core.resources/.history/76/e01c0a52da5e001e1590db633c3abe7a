#include <stdio.h>


int pares(){

	int v[5], n = 0;

	for(int i = 0; i < 10; i++){

		printf("Digite o %dÂº numero: ", i + 1);
		scanf("%d", &v[i]);
		printf("\n");

	}for(int i = 0; i < 10; i++){

		if(v[i] % 2 == 0){

			n += 1;
		}

	}printf("O vetor tem %d numeros pares", n);

}


int main(){

	pares();

	return 0;

}
