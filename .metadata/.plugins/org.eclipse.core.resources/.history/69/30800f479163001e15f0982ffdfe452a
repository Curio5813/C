#include <stdio.h>


void vetor_uniao(){

	int a[10], b[10], u[10], i, k, cont = 0;

	for(i = 0; i < 10; i++){
		printf("Digite o %dº inteiro para o vetor A: ", i + 1);
		scanf("%d", &a[i]);

	for(i = 0; i < 10; i++){
		printf("Digite o %dº inteiro para o vetor B: ", i + 1);
		scanf("%d", &b[i]);


	}for(i = 0; i < 10; i ++){
		for(k = 0; k < 10; k++){
			if(a[i] == b[k]){
				u[i] = a[i];
				cont += 1;
			}

		}if(cont == 0){
			u[i] = 0;
			cont = 0;
		}

	}for(i = 0; i < 10; i++){
		if(u[i] != 0){
			printf("%d ", u[i]);

		}

	}

}


int main(){

	vetor_uniao();

	return 0;

}
