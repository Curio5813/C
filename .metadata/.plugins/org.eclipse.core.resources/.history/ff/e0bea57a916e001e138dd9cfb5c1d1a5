#include <stdio.h>
#include <stdlib.h>


int array_e_ponteiros(int *v);

	int valor = 2, *p = &valor;

	a = *(p);

	v[0] = a;
	++a;
	v[1] = a;
	++a;
	v[2] = a;
	++a;
	v[3] = a;
	++a;
	v[4] = a;


int main(){

	int v[5];

	array_e_ponteiros(&v);

	for(int i = 0; i < 5; i++){

		printf("%d ", v[i]);

	}

	return 0;

}
