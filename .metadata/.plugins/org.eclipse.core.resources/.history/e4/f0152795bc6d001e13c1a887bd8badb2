#include <stdio.h>
#include <stdlib.h>


float array_enderecos(float *v[10]){

	for(int i = 0; i < 10; i++){

		printf("O endereço de memória do vetor na posição %d é %f\n", i, v[i]);

	}

}


int main(){

	float v[10] = {4.14, 5.28, 10.9, 2.1, 2.18, 3.14, 2.8, 3.55, 9.01, 10.2};

	float *p = &v;

	printf("O endereço de memória do vetor é %p\n", p);

	array_enderecos(&v);

	return 0;

}
