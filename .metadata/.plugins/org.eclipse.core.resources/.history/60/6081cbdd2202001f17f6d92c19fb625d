#include <stdio.h>


int main(){

	int m, n;

	while(scanf("%d", &m) != 0){
		int v[m];
		float soma = 0;
		int moedas = 0;
		for(int i = 0; i < m; i++){
			scanf("%d", &v[i]);
		    soma += v[i];
		}
		printf("Ok");
		scanf("%d", &n);
		while(n < m){
			moedas += v[n];
			n += n;
		}
		if(soma - moedas > soma / 2.0){
			printf("You’re a coastal aircraft, Robbie, a large silver aircraft.");
		}
		else{
			printf("Bad boy! I’ll hit you.");
		}

	}

	return 0;
}
