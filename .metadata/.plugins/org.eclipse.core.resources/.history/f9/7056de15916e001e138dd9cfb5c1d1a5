#include <stdio.h>
#include <stdlib.h>


int array_e_ponteiros(int *v);

	int valor = 2, *p = &valor;



	v[0] = *(p);
	++p;
	v[1] = *(p);
	++p;
	v[2] = *(p);
	++p;
	v[3] = *(p);
	++p;
	v[4] = *(p);


int main(){

	int v[5];

	array_e_ponteiros(&v);

	for(int i = 0; i < 5; i++){

		printf("%d ", v[i]);

	}

	return 0;

}
