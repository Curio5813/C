#include <stdio.h>
#include <stdlib.h>
#include <string.h>


int main(){

	char str_col[26];

	int idx;

	scanf("%s", &str_col);
	while(strlen(str_col) > 3){
		printf("Essa coluna nao existe Tobias!");
		scanf("%s", &str_col);

	}if(strlen(str_col) == 1){
		printf("%d\n", str_col[0] - 64);

	}else if(strlen(str_col) == 2){
		printf("%d\n", str_col[1] - 64);

	}else if(strlen(str_col) == 3){
		printf("%d\n", str_col[2] - 64)

	}

	return 0;

}
