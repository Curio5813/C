#include <stdio.h>


void superfatorial(){

	long long int sf = 1, fat = 1, n = 1, num;

	printf("Digite um número para calcular seu perfatorial: ");
	scanf("lld", &num);
	printf("\n");

	for(int i = 1; i <= num; i++){
		while(n <= i){
			fat *= n;
			n += 1;

		}sf *= fat;
		fat = 1;
		n = 1;

	}printf("O superfatorial de %lld é %lld.", num, sf);

}


int main(){

	superfatorial();

	return 0;

}
